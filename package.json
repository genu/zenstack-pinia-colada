    "@zenstackhq/schema": "3.1.0"
  },
  "devDependencies": {
    "@pinia/colada": "^0.20.0",
    "@types/node": "^25.0.3",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "^3.7.4",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "vue": "^3.5.26"
  },
  "peerDependencies": {
    "@pinia/colada": ">=0.10.0 <1.0.0",
