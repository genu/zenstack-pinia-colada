{
  "name": "zenstack-pinia-colada",
  "version": "0.1.4",
  "description": "Pinia Colada Client for consuming ZenStack v3's CRUD service",
  "type": "module",
  "sideEffects": false,
  "packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40",
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:generate": "tsx scripts/generate.ts",
    "test:typecheck": "tsc --noEmit --project tsconfig.test.json",
    "prepublishOnly": "pnpm run build",
    "prepack": "pnpm run build",
    "changelogen": "changelogen",
    "release": "changelogen --release && npm publish",
    "release:push-tags": "git push --follow-tags"
  },
  "keywords": [
    "zenstack",
    "pinia-colada",
    "pinia",
    "vue",
    "orm",
    "fullstack",
    "data-fetching"
  ],
  "author": "ZenStack Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/genu/zenstack-pinia-colada.git"
  },
  "bugs": {
    "url": "https://github.com/genu/zenstack-pinia-colada/issues"
  },
  "homepage": "https://github.com/genu/zenstack-pinia-colada#readme",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@zenstackhq/common-helpers": "^3.0.0-beta.26",
    "@zenstackhq/orm": "^3.0.0-beta.26",
    "@zenstackhq/schema": "^3.0.0-beta.26",
    "decimal.js": "^10.6.0",
    "superjson": "^2.2.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@pinia/colada": "^0.17.9",
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^24.10.1",
    "@zenstackhq/language": "^3.0.0-beta.26",
    "@zenstackhq/sdk": "^3.0.0-beta.26",
    "changelogen": "^0.6.2",
    "eslint": "~9.39.1",
    "glob": "^13.0.0",
    "happy-dom": "^20.0.11",
    "nock": "^14.0.10",
    "pinia": "^3.0.4",
    "prettier": "^3.7.3",
    "tsdown": "^0.16.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14",
    "vue": "^3.5.25"
  },
  "peerDependencies": {
    "@pinia/colada": ">=0.10.0 <1.0.0",
    "pinia": "^3.0.0",
    "vue": "^3.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
